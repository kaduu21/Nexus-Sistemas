<?php

class Aluno {
    private string $matricula;
    private string $nome;
    private int $idade;
    private string $sexo;
    private string $email;
    private string $curso;

    public function getMatricula()
    {
        return $this -> matricula;
    }

    public function setMatricula()
    {
        $this -> matricula = $matricula;
    }

    public function getNome()
    {
        return $this -> nome;
    }

    public function setNome()
    {
        $this -> nome = $nome;
    }

    public function getIdade()
    {
        return $this -> idade;
    }

    public function setIdade()
    {
        $this -> idade = $idade;
    }

    public function getSexo()
    {
        return $this -> sexo;
    }

    public function setSexo()
    {
        $this -> sexo = $sexo;
    }

    public function getEmail()
    {
        return $this -> email;
    }

    public function setEmail()
    {
        $this -> email = $email;
    }

    public function getCurso()
    {
        return $this -> curso;
    }

    public function setCurso()
    {
        $this -> curso = $curso;
    }


}

class Funcionario {
    private string $ID;
    private string $nome;
    private string $idade;
    private string $sexo;
    private string $email;
    private string $graduacao;

    public function getID()
    {
        return $this -> ID;
    }

    public function setMatricula()
    {
        $this -> ID = $ID;
    }

    public function getNome()
    {
        return $this -> nome;
    }

    public function setNome()
    {
        $this -> nome = $nome;
    }

    public function getIdade()
    {
        return $this -> idade;
    }

    public function setIdade()
    {
        $this -> idade = $idade;
    }

    public function getSexo()
    {
        return $this -> sexo;
    }

    public function setSexo()
    {
        $this -> sexo = $sexo;
    }

    public function getEmail()
    {
        return $this -> email;
    }

    public function setEmail()
    {
        $this -> email = $email;
    }

    public function getGraduacao()
    {
        return $this -> graduacao;
    }

    public function setGraduacao()
    {
        $this -> graduacao = $graduacao;
    }

}

class Professor extends Funcionario {
    private string $turma;
    private string $disciplina;

    public function getTurma()
    {
        return $this -> turma;
    }

    public function setTurma()
    {
        $this -> turma = $turma;
    }

    public function getDisciplina()
    {
        return $this -> disciplina;
    }

    public function setDisciplina()
    {
        $this -> disciplina = $disciplina;
    }

}

class Coordenacao extends Funcionario {
    private string $areaAtuacao;

    public function getAreaAtuacao()
    {
        return $this -> areaAtuacao;
    }

    public function setAreaAtuacao()
    {
        $this -> areaAtuacao = $areaAtuacao;
    }

}
